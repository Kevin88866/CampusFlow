{
  "name": "myproject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "patch-package",
    "test:backend": "cross-env NODE_ENV=test DATABASE_URL=postgres://postgres:postgres@localhost:5433/campusflow_test jest --config=jest.backend.config.js --runInBand",
    "test:fe": "jest --config=jest.config.js",
    "test:be": "jest --config=jest.backend.config.js --runInBand",
    "test:coverage:fe": "jest --config=jest.config.js --coverage",
    "test:coverage:be": "jest --config=jest.backend.config.js --coverage",
    "test:coverage": "npm run test:coverage:fe && npm run test:coverage:be",
    "db:test:reset": "psql -h localhost -p 5433 -U postgres -d campusflow_test -f testdb/schema.sql && psql -h localhost -p 5433 -U postgres -d campusflow_test -f testdb/seed.sql"
  },
